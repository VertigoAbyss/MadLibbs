<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Input_Page">

    <EditText
        android:id="@+id/textViewInLists"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Occupation"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="281dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:onClick="sendMessage"
        android:text="Submit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists8" />

    <EditText
        android:id="@+id/textViewInLists6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Ridiculous Name"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists5" />

    <EditText
        android:id="@+id/textViewInLists2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Professor Name"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists" />

    <EditText
        android:id="@+id/textViewInLists5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Object (singular)"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists4" />

    <EditText
        android:id="@+id/textViewInLists3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Object (plural)"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists2" />

    <EditText
        android:id="@+id/textViewInLists7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Object (plural)"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists6" />

    <EditText
        android:id="@+id/textViewInLists4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Strong Emotion"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists3" />

    <EditText
        android:id="@+id/textViewInLists8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:fontFamily="monospace"
        android:hint="Different Occupation"
        android:padding="5dp"
        android:text=""
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textColor="@color/ePurpD"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@+id/textViewInLists7" />

</androidx.constraintlayout.widget.ConstraintLayout>